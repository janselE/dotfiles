# alias
alias vim='nvim'
alias v='nvim'
alias cp='cp -rv'
alias p='python'
alias rm='rm -rf'
alias g='git'
alias ga='git add'
alias gcm='git commit -m'
alias gp='git push'
alias gll='git pull'
alias la='ls -a'

alias t='tmux'
alias ta='tmux a -t'
alias tls='tmux ls'
alias tn='tmux new -t'

alias python=python3
alias node=nodejs

# from bashrc
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# ubuntu
alias open='nautilus'
alias zathura='zathura --fork'
